{
  "name": "stream-stash-frontend",
  "version": "0.0.1",
  "description": "Companion for streaming movies, TV shows and anime",
  "productName": "Stream Stash",
  "author": "Enrico Koschel <enricokoschel02@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "lint-pre-commit": "eslint --ext .js,.ts,.vue --rule \"@typescript-eslint/no-unused-vars: error\" --rule \"vue/no-unused-vars: error\" ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "lint-pre-commit",
    "format"
  ],
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.6.7",
    "axios-rate-limit": "^1.3.0",
    "pinia": "^2.0.11",
    "quasar": "^2.14.5",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0",
    "vue3-google-login": "^2.0.26"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.7.3",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.21.1",
    "pre-commit": "^1.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "dotenv": "^16.4.5"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
